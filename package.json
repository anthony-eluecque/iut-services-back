{
  "name": "iut-services-back",
  "version": "1.0.0",
  "description": "",
  "main": "./server.ts",
  "type": "commonjs",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "dev": "nodemon server.ts",
    "start": "ts-node src/index.ts",
    "lint": "eslint ./src/**/*.ts --fix",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -d ./src/config",
    "docker": "nodemon -L -e ts --exec 'tsc -p ./tsconfig.json && node ./dist/server.js'",
    "migration:create": "ts-node -r tsconfig-paths/register node_modules/typeorm/cli migration:create src/migrations/%npm_config_name%",
    "migration:generate": "npm run typeorm -- migration:generate ./src/migrations/$npm_config_name",
    "migration:revert": "npm run typeorm -- migration:revert",
    "migration:run": "npm run typeorm -- migration:run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anthony-eluecque/iut-services-back.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/anthony-eluecque/iut-services-back/issues"
  },
  "homepage": "https://github.com/anthony-eluecque/iut-services-back#readme",
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.6",
    "@types/md5": "^2.3.3",
    "@types/supertest": "^2.0.15",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "eslint": "^8.48.0",
    "eslint-config-standard-with-typescript": "^39.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.0.2",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/crypto-js": "^4.1.2",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.3.1",
    "dotenv-display": "^1.0.1",
    "express": "^4.17.1",
    "express-validator": "^7.0.1",
    "generate-password": "^1.7.1",
    "install": "^0.13.0",
    "jsonwebtoken": "^9.0.2",
    "md5": "^2.3.0",
    "nodemailer": "^6.9.7",
    "npm": "^10.1.0",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.1.6",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "ts-node": "^10.9.1",
    "typeorm": "0.3.17"
  }
}
